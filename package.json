{
  "name": "spt-11",
  "version": "1.0.0",
  "description": "Just an assignment",
  "main": "webpack.config.js",
  "config": {
    "port": 5000
  },
  "scripts": {
    "build:css": "stylus -c www/index.styl -o www/~index.css",
    "build:tag": "riot --config riot.config.js",
    "build:js": "browserify -e www/index.js -o www/~bundle.js -d -t [ babelify ] -p [minifyify --output www/~bundle.js.map]",
    "build:js-1": "browserify -e www/index.js -o www/~bundle.js -d -t [ babelify ]",
    "build": "npm run build:tag && npm run build:js && npm run build:css",
    "prestart": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "highlight.js": "isagalaev/highlight.js.git",
    "jquery": "^3.0.0",
    "nib": "^1.1.0",
    "open": "0.0.5",
    "riot": "^2.4.1",
    "stylus": "^0.54.5"
  },
  "engines": {
    "node": "^6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/userpixel/spt-11.git"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-riot": "^1.1.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "minifyify": "^7.3.3"
  }
}
